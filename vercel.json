{
  "rewrites": [
    {
      "source": "/scaffold-diffusion",
      "destination": "https://scaffold-diffusion-production.up.railway.app"
    },
    {
      "source": "/scaffold-diffusion/:path*",
      "destination": "https://scaffold-diffusion-production.up.railway.app/:path*"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "**": {
      "runtime": "@vercel/node@18.x"
    }
  }
}